{
  "name": "vue-prefetch",
  "version": "0.0.1",
  "description": "data preloading solution for vue project",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "browser": "umd/sdp-vue-prefetch.umd.js",
  "author": "sdpfe-group",
  "scripts": {
    "clean": "rimraf build",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:umd": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:umd && npm run copy:files",
    "test": "NODE_ENV=test jest",
    "copy:files": "cpx 'package.json LICENSE' build/",
    "deploy": "cd build &&  npm publish",
    "deploy:local": "cd build && npm link"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "vue2",
    "assets prefetch",
    "data preload",
    "vue3",
    "react"
  ],
  "peerDependencies": {
    "babel-core": "6.26.3",
    "webpack": "^5",
    "babel-loader": ">=7.1.5",
    "vue-loader": ">=14.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^26.0.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-sources": "^3.2.3",
    "@vue/test-utils": "^1.0.0",
    "babel-jest": "^26.0.0",
    "cpx": "^1.5.0",
    "jest": "^26.0.0",
    "rimraf": "^5.0.7",
    "ts-loader": "^9.0.0",
    "typescript": "^4.9.5",
    "vue-jest": "^3.0.0",
    "vue-loader": "^14.2.4",
    "vue-template-compiler": "^2.6.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.0",
    "webpack-sources": "^3.2.3"
  },
  "engines": {
    "node": "14.x"
  },
  "engine-strict": true,
  "license": "MIT"
}
